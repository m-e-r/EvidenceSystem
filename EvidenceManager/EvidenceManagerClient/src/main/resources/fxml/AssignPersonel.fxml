<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane id="AnchorPane" prefHeight="521.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="FXMLEntity.FXMLAssignPersonelController">
   <children>
      <VBox prefHeight="521.0" prefWidth="872.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="searchTF" prefHeight="23.0" prefWidth="600.0" promptText="Search (id, name or rank)">
               <VBox.margin>
                  <Insets left="100.0" right="100.0" />
               </VBox.margin>
            </TextField>
            <TableView fx:id="usersTV" prefHeight="200.0" prefWidth="200.0">
               <columns>
                  <TableColumn fx:id="TVidCol" minWidth="-1.0" prefWidth="166.0" text="Id" />
                  <TableColumn fx:id="TVNameCol" minWidth="-1.0" prefWidth="240.0" text="Name" />
                  <TableColumn fx:id="TVRankCol" minWidth="-1.0" prefWidth="193.0" text="Rank" />
               </columns>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </TableView>
            <HBox alignment="CENTER" prefHeight="227.0" prefWidth="600.0" spacing="70.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="40.0">
                     <children>
                        <Button fx:id="addBTN" mnemonicParsing="false" onAction="#handleAddAction" text="Add" />
                        <Button fx:id="removeBTN" mnemonicParsing="false" onAction="#handleRemoveAction" text="Remove" />
                     </children>
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="40.0">
                     <children>
                        <Button fx:id="addAllBTN" mnemonicParsing="false" onAction="#handleAddAllAction" text="Add all" />
                        <Button fx:id="clearAllBTN" mnemonicParsing="false" onAction="#handleClearAllAction" text="Clear all" />
                     </children>
                     <HBox.margin>
                        <Insets top="50.0" />
                     </HBox.margin>
                  </VBox>
                  <ListView fx:id="selectedLV" prefHeight="200.0" prefWidth="200.0">
                     <HBox.margin>
                        <Insets top="12.0" />
                     </HBox.margin>
                  </ListView>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets top="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
