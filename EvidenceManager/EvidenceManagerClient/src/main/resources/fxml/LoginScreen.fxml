<AnchorPane id="AnchorPane" prefHeight="268.0" prefWidth="465.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXMLUser.FXMLLoginController">
   <children>
      <VBox alignment="CENTER" prefHeight="268.0" prefWidth="465.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0">
               <children>
                  <Label text="Brugernavn" />
                  <TextField fx:id="userNameTF" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" />
                  <Label fx:id="loginLabel" textFill="#c94343d6">
                     <font>
                        <Font name="System Italic" size="13.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0">
               <children>
                  <Label text="Kodeord" />
                  <TextField fx:id="passwordTF" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="180.0" />
                  <Button fx:id="loginBTN" mnemonicParsing="false" onAction="#handleLoginAction" text="Log ind" />
               </children>
               <padding>
                  <Insets bottom="20.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
